{
  "name": "cosmeticweb",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "publish-coverage": "nyc report --reporter=text-lcov | coveralls",
	"lint": "esw  ./test ./models/ app.js",
    "lint:watch": "npm run lint --watch",
    "test:watch": "npm run test --watch",
    "server:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "clean": "rimraf ./lib && mkdir lib",
    "server": "cross-env NODE_ENV=dev PORT=3001 node lib/bin/www",
    "start": "npm-run-all test build server",
	"coverage": "cross-env NODE_ENV=test PORT=4000 nyc mocha test/routes/cosmetics-test.js --timeout 5000",
    "coverage_cosmetic": "cross-env NODE_ENV=test PORT=4000 nyc mocha test/routes/cosmetics-test.js --timeout 5000",
	"coverage_customer": "cross-env NODE_ENV=test PORT=4000 nyc mocha test/routes/customers-test.js --timeout 5000",
	"coverage_seller": "cross-env NODE_ENV=test PORT=4000 nyc mocha test/routes/sellers-test.js --timeout 5000",
	"coverage_transaction": "cross-env NODE_ENV=test PORT=4000 nyc mocha test/routes/transactions-test.js --timeout 5000",
	"coverage_user_image": "cross-env NODE_ENV=test PORT=4000 nyc mocha test/routes/user_images-test.js --timeout 5000",
    "test": "npm-run-all test_cosmetic test_customer test_seller test_transaction test_user_image",
    "test_cosmetic": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register test/routes/cosmetics-test.js",
    "test_customer": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register test/routes/customers-test.js",
    "test_seller": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register test/routes/sellers-test.js",
    "test_transaction": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register test/routes/transactions-test.js",
    "test_user_image": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register test/routes/user_images-test.js",
    "postbuild": "npm run server",
    "prebuild": "npm-run-all test clean",
    "build": "babel ./ --out-dir lib/ --ignore ./node_modules,./.babelrc,./package.json,./package-lock.json,./test --copy-files"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "chai-things": "^0.2.0",
    "config": "^2.0.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "joi": "^14.0.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "mongodb": "^3.1.8",
    "mongoose": "^5.3.2",
    "morgan": "~1.9.0",
    "multer": "^1.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-plugin-json": "^1.3.2",
    "eslint-watch": "^4.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "webpack-cli": "^3.1.2"
  }
}
